  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "542f2732-fe4f-4521-ac52-8f781de2f29e",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['Sex'] = df['Sex'].map({0: 'male', 1: 'female'})\n",
    "sex_encoded = pd.get_dummies(df['Sex'], drop_first=True)    \n",
    "embarked_encoded = pd.get_dummies(df['Embarked'], prefix='Emb', drop_first=True)\n",
    "df = pd.concat([df, sex_encoded, embarked_encoded], axis=1)\n",
    "df.drop(['Sex', 'Embarked'], axis=1, inplace=True)"
   ]
